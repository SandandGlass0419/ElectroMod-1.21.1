{
  "multipart": [
    { "when": { "facing": "up" },    "apply": { "model": "electromod:block/copper_wire" } },
    { "when": { "facing": "down" },  "apply": { "model": "electromod:block/copper_wire", "x": 180 } },
    { "when": { "facing": "north" }, "apply": { "model": "electromod:block/copper_wire_dot", "x": 90 } },
    { "when": { "facing": "south" }, "apply": { "model": "electromod:block/copper_wire_dot", "x": 90, "y": 180 } },
    { "when": { "facing": "east" },  "apply": { "model": "electromod:block/copper_wire_dot", "x": 90, "y": 90 } },
    { "when": { "facing": "west" },  "apply": { "model": "electromod:block/copper_wire_dot", "x": 90, "y": 270 } },

    { "when": { "facing": "up",    "north": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 0,   "y": 0   } },
    { "when": { "facing": "up",    "east":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 0,   "y": 90  } },
    { "when": { "facing": "up",    "south": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 0,   "y": 180 } },
    { "when": { "facing": "up",    "west":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 0,   "y": 270 } },

    { "when": { "facing": "down",  "north": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 180, "y": 0   } },
    { "when": { "facing": "down",  "east":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 180, "y": 90  } },
    { "when": { "facing": "down",  "south": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 180, "y": 180 } },
    { "when": { "facing": "down",  "west":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 180, "y": 270 } },

    { "when": { "facing": "north", "north": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 0   } },
    { "when": { "facing": "north", "east":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 90  } },
    { "when": { "facing": "north", "south": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 180 } },
    { "when": { "facing": "north", "west":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 270 } },

    { "when": { "facing": "south", "north": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 270, "y": 0   } },
    { "when": { "facing": "south", "east":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 270, "y": 90  } },
    { "when": { "facing": "south", "south": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 270, "y": 180 } },
    { "when": { "facing": "south", "west":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 270, "y": 270 } },

    { "when": { "facing": "east",  "north": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 90  } },
    { "when": { "facing": "east",  "east":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 180 } },
    { "when": { "facing": "east",  "south": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 270 } },
    { "when": { "facing": "east",  "west":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 0   } },

    { "when": { "facing": "west",  "north": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 270 } },
    { "when": { "facing": "west",  "east":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 0   } },
    { "when": { "facing": "west",  "south": true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 90  } },
    { "when": { "facing": "west",  "west":  true }, "apply": { "model": "electromod:block/copper_wire_side", "x": 90, "y": 180 } }
  ]
}
